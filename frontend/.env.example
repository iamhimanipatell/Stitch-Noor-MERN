# Frontend Environment Variables - Example
# Copy this file to .env.local and fill in your actual values

# API Configuration
REACT_APP_API_URL=https://api.yourdomain.com
REACT_APP_ADMIN_URL=https://admin.yourdomain.com

# Environment
REACT_APP_ENV=production

# Admin Credentials (for demo purposes)
REACT_APP_ADMIN_EMAIL=admin@yourdomain.com
REACT_APP_ADMIN_PASSWORD=your-secure-password

# App Configuration
REACT_APP_APP_NAME=StitchNoor
REACT_APP_APP_VERSION=1.0.0